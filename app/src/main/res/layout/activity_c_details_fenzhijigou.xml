<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="10dp">
    <LinearLayout
        android:id="@+id/fenzhijiegou"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/details_con_bg"
        android:orientation="vertical"
        android:layout_marginBottom="20dp"
        android:padding="10dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="序号："
            android:textColor="#000000"
            android:textStyle="bold"
            android:textSize="@dimen/fab_margin" />
        <TextView
            android:id="@+id/fz_tv1CXH"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1"
            android:textColor="#000000"
            android:textStyle="bold"
            android:textSize="@dimen/fab_margin" />

        <View
            android:background="#B8B8B8"
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="1dp"/>
        <TextView
            android:id="@+id/fz_tv1CX1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="注册号/统一社会信用代码："
            android:textStyle="bold"
            android:textColor="#000000"
            android:textSize="@dimen/ctitle_size" />

        <TextView
            android:id="@+id/fz_tv1C"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:textStyle="bold"
            android:text="纳税先锋"
            android:textColor="#57737D"
            android:textSize="@dimen/ctest_size" />

        <View
            android:background="#B8B8B8"
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="1dp"/>
        <TextView
            android:id="@+id/fz_tv1CX2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:textStyle="bold"
            android:text="名称："
            android:textColor="#000000"
            android:textSize="@dimen/ctitle_size" />

        <TextView
            android:id="@+id/fz_tv2C"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="国家级"
            android:textStyle="bold"
            android:textColor="#57737D"
            android:textSize="@dimen/ctest_size" />
        <View
            android:background="#B8B8B8"
            android:layout_width="match_parent"
            android:layout_margin="5dp"
            android:layout_height="1dp"/>

        <TextView
            android:id="@+id/fz_tv1CX3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="登记机关："
            android:textStyle="bold"
            android:textColor="#000000"
            android:textSize="@dimen/ctitle_size" />

        <TextView
            android:id="@+id/fz_tv3C"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="江西省工商银行"
            android:textStyle="bold"
            android:textColor="#57737D"
            android:textSize="@dimen/ctest_size" />

    </LinearLayout>

</LinearLayout>
